{
    "manifest_version":1,
    "name":"Underscore",
    "version":"0.0.1",
    "content_scripts":[{
        "matches":[
            "<all_urls>"
        ],
        "js":["content.js"]
    }],
    "background":{
        "scripts":["background.js"]
    },
    "permissions":["tabs","background","storage"],
    "browser_action":{
        "default_icon":"logo.png",
        "default_popup":"popup.html",
        "default_title":"Underscore_"
    }
}