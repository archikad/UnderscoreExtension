{
    "manifest_version":2,
    "name":"Underscore",
    "version":"2.0",
    "content_scripts":[{
        "matches":[
            "<all_urls>"
        ],
        "js":["content.js"]
    }],
    "background":{
        "scripts":["background.js"]
    },
    "permissions":["tabs","background","storage"],
    "browser_action":{
        "default_icon":"news.png",
        "default_popup":"popup.html",
        "default_title":"Underscore_ Extension"
    }
}